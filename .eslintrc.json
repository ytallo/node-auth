{
    "extends": ["airbnb-base", "prettier"],
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "plugins": ["prettier"],
    "rules": {
      "no-param-reassign": ["error", { "props": false }],
      "class-methods-use-this": "off"
    }
  }
  